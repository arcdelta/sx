{
  "name": "x-s",
  "version": "1.0.0",
  "description": "Micro CLI for interactive listing & running npm scripts.",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "x-s": "./bin/x-s"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -cw",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "install:glob": "npm run build && npm i -g"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcdelta/x-s.git"
  },
  "keywords": [
    "cli",
    "node",
    "interactive",
    "npm-scripts",
    "npm"
  ],
  "author": {
    "name": "Vladislav Mamon",
    "email": "hello@arcdelta.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arcdelta/x-s/issues"
  },
  "homepage": "https://github.com/arcdelta/x-s#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.13.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "rollup": "^2.9.0",
    "semantic-release": "^17.0.7"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "commander": "^5.1.0",
    "enquirer": "^2.3.5",
    "fs-extra": "^9.0.0"
  }
}
